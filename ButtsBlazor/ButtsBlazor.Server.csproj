<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Platforms>AnyCPU</Platforms>
    <UserSecretsId>4af0d564-7a0f-4be3-9d69-5546e3105ed1</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerComposeProjectPath>..\docker-compose.dcproj</DockerComposeProjectPath>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <PropertyGroup Label="Configuration">
    <TypeScriptTarget>ES2020</TypeScriptTarget>
    <TypeScriptNoImplicitAny>True</TypeScriptNoImplicitAny>
    <TypeScriptGeneratesDeclarations>False</TypeScriptGeneratesDeclarations>
    <TypeScriptOutDir>/</TypeScriptOutDir>
    <TypeScriptCompileOnSaveEnabled>False</TypeScriptCompileOnSaveEnabled>
    <TypeScriptToolsVersion>5.3</TypeScriptToolsVersion>
    <TypeScriptCompileBlocked>true</TypeScriptCompileBlocked>
    <TypeScriptModuleKind>none</TypeScriptModuleKind>
  </PropertyGroup>


  <ItemGroup>
    <Compile Remove="sampleSdXl\**" />
    <Compile Remove="Scripts\prompts\node_modules\**" />
    <Compile Remove="wwwroot\butts_images\**" />
    <Compile Remove="wwwroot\ts\**" />
    <Compile Remove="wwwroot\ts\Scripts\prompts\%40gradio\**" />
    <Content Remove="sampleSdXl\**" />
    <Content Remove="Scripts\prompts\node_modules\**" />
    <Content Remove="wwwroot\butts_images\**" />
    <Content Remove="wwwroot\ts\**" />
    <Content Remove="wwwroot\ts\Scripts\prompts\%40gradio\**" />
    <EmbeddedResource Remove="sampleSdXl\**" />
    <EmbeddedResource Remove="Scripts\prompts\node_modules\**" />
    <EmbeddedResource Remove="wwwroot\butts_images\**" />
    <EmbeddedResource Remove="wwwroot\ts\**" />
    <EmbeddedResource Remove="wwwroot\ts\Scripts\prompts\%40gradio\**" />
    <None Remove="sampleSdXl\**" />
    <None Remove="Scripts\prompts\node_modules\**" />
    <None Remove="wwwroot\butts_images\**" />
    <None Remove="wwwroot\ts\**" />
    <None Remove="wwwroot\ts\Scripts\prompts\%40gradio\**" />
  </ItemGroup>


  <ItemGroup>
    <Content Remove="wwwroot\js\worker\utils.d.ts" />
    <Content Remove="wwwroot\worker\loadButtsWorker.d.ts" />
    <Content Remove="wwwroot\worker\utils.d.ts" />
  </ItemGroup>


  <ItemGroup>
    <None Remove="Scripts\prompts\documentListener.ts" />
    <None Remove="Scripts\prompts\prompt.ts" />
  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\ButtsBlazor.Api\ButtsBlazor.Api.csproj" />
    <ProjectReference Include="..\ButtsBlazor.Client\ButtsBlazor.Client.csproj" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Server" Version="8.0.0-rc.2.23480.2" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation" Version="8.0.0-rc.2.23480.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.0-rc.2.23480.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="8.0.0-rc.2.23480.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="8.0.0-rc.2.23480.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="8.0.0-rc.2.23479.6" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.19.5" />
    <PackageReference Include="Nito.AsyncEx.Coordination" Version="5.1.2" />
  </ItemGroup>

  <ItemGroup>
    <TypeScriptCompile Include="Scripts\prompts\documentListener.ts" />
    <TypeScriptCompile Include="Scripts\prompts\prompt.ts" />
    <TypeScriptCompile Include="Worker\loadButtsWorker.ts" />
    <TypeScriptCompile Include="wwwroot\js\worker\loadButtsWorker.d.ts" />
    <TypeScriptCompile Include="wwwroot\js\worker\utils.d.ts" />
    <TypeScriptCompile Include="wwwroot\worker\loadButtsWorker.d.ts" />
    <TypeScriptCompile Include="wwwroot\worker\utils.d.ts" />
  </ItemGroup>

  <ItemGroup>
    <Content Update="wwwroot\js\worker\loadButtsWorker.js">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Update="wwwroot\js\worker\loadButtsWorker.js.map">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Update="wwwroot\js\worker\tsconfig.tsbuildinfo">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Update="wwwroot\js\worker\utils.js">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Update="wwwroot\js\worker\utils.js.map">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>

</Project>
